var ui=JavaImporter(org.openqa.selenium.support.ui) //import Selenium Support UI package
var wait=new support_ui.WebDriverWait(WDS.browser,5000) // wait 5 seconds until page loads

WDS.sampleResult.sampleStart() //sample starting point
WDS.browser.get(&apos;http://10.10.60.52/9.1.1/MyESOPs/&apos;)

var pkg = JavaImporter(org.openqa.selenium)



var username = WDS.vars.get(&apos;UN&apos;)
var User=WDS.browser.findElement(pkg.By.id(&quot;txtUserId&quot;))
User.sendKeys(username)

var password = WDS.vars.get(&apos;PWD&apos;)
var Password=WDS.browser.findElement(pkg.By.id(&quot;txtPassword&quot;))
Password.sendKeys(password)

var companyName = WDS.vars.get(&apos;CN&apos;)
 var CompanyID=WDS.browser.findElement(pkg.By.id(&quot;txtCompanyId&quot;))
 CompanyID.sendKeys(companyName)
WDS.log.info(&quot;username :&quot;+username)
WDS.log.info(&quot;Password :&quot;+password)
WDS.log.info(&quot;Password :&quot;+companyName)

 var Login=WDS.browser.findElement(pkg.By.id(&quot;login_btn&quot;))
 Login.click()
java.lang.Thread.sleep(3000)
 
</stringProp>
